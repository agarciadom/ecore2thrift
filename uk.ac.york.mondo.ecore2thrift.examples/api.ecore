        </eAnnotations>
      </eParameters>
      <eParameters name="profile" eType="#//UserProfile">
        <eAnnotations source="doc">
          <details key="d" value="The updated profile of the user"/>
        </eAnnotations>
      </eParameters>
    </eOperations>
